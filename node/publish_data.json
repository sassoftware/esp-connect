[
    {
        "info": {
            "defaults": {
                "dateformat":"",
                "dataformat":"csv",
                "blocksize":"1",
                "times":"1"
            },

            "usage":{
                "usage":"-window -events [-dataformat] [-blocksize]",
                "options":{
                    "window":"ESP source window in the form of project/contquery/window",
                    "events":"file containing the ESP events",
                    "dataformat":"csv | xml | json (the format of the event data, defaults to csv)",
                    "blocksize":"event block size (defaults to 1)",
                    "dateformat":"event date format"
                }
            },
            "exit-on-response":true
        }
    },

    {
        "data-publisher": {
            "id":"#guid#",
            "window":"@window@",
            "data":"$events$",
            "blocksize":"@blocksize@",
            "informat":"@dataformat@",
            "dateformat":"@dateformat@",
            "times":"@times@"
        }
    }
]
