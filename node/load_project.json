[
    {
        "info": {
            "usage":{
                "usage":"-name -model [-connectors] [-overwrite] [-validate] [-start]",
                "options":{
                    "name":"name of the ESP project",
                    "model":"file containing the ESP model",
                    "connectors":"true | false (start connectors, defaults to true)",
                    "overwrite":"true | false (overwrite project if it exists, defaults to false)",
                    "validate":"true | false (validate the project, defaults to true)",
                    "start":"true | false (start the project, defaults to true)"
                }
            },
            "defaults": {
                "connectors":"true",
                "overwrite":"false",
                "validate":"true",
                "start":"true"
            },
            "exit-on-response":true
        }
    },
    {
        "project": {
            "name":"@name@",
            "data":"$model$",
            "action":"load",
            "connectors":"@connectors@",
            "overwrite":"@overwrite@",
            "validate":"@validate@",
            "start":"@start@",
            "#include#":"#parms#"
        }
    }
]
