[
    {
        "info": {
            "usage":{
                "usage":"[-name] [-format]",
                "options":{
                    "name":"name of the project for which to retrieve the model (defaults to all)",
                    "format":"xml | json | ubjson (defaults to xml)",
                    "schema":"true | false (return schema, defaults to false)"
                }
            },
            "defaults": {
                "name":"",
                "format":"xml",
                "schema":"no"
            },
            "exit-on-response":true
        }
    },
    {
        "model": {
            "name":"@name@",
            "format":"@format@",
            "schema":"@schema@"
        }
    }
]
