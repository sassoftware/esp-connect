[
    {
        "info": {
            "defaults": {
                "blocksize":"1",
                "dateformat":"",
                "times":"1",
                "informat":""
            },

            "usage":{
                "usage":"-window -url [-informat] [-blocksize]",
                "options":{
                    "window":"ESP source window in the form of project/contquery/window",
                    "url":"URL containing event data",
                    "informat":"csv | xml | json | bin (input data format, default is to derive from the URL)",
                    "blocksize":"event block size (defaults to 1)",
                    "dateformat":"event date format",
                    "times":"number of times to publish (defaults to 1)"
                }
            },
            "exit-on-response":true
        }
    },

    {
        "url-publisher": {
            "id":"#guid#",
            "window":"@window@",
            "url":"@url@",
            "informat":"@informat@",
            "blocksize":"@blocksize@",
            "dateformat":"@dateformat@",
            "times":"@times@"
        }
    }
]
